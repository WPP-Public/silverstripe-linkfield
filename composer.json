{
  "name": "gorriecoe/silverstripe-linkfield",
  "description": "Adds a Linkfield for gorriecoe/silverstripe-link",
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Gorrie Coe",
      "email": "gorriecoe@gmail.com"
    }
  ],
  "keywords": [
    "silverstripe",
    "modular",
    "cms",
    "link"
  ],
  "homepage": "http://github.com/gorriecoe/silverstripe-linkfield",
  "require": {
    "gorriecoe/silverstripe-link": "dev-master",
    "silvershop/silverstripe-hasonefield": "dev-main",
    "symbiote/silverstripe-gridfieldextensions": "^5"
  },
  "extra": {
    "installer-name": "linkfield",
    "expose": [
      "client/dist"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:WPP-Public/silverstripe-link.git"
    }
  ]
}
